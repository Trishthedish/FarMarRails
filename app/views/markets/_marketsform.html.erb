<%= form_for @mymarket,
  method: @market_method,
  url: @market_path do |f| %>
  <%= f.label :name %>
    <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :county %>
  <%= f.text_field :county %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>
    <% end %>