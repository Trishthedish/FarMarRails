 <h1>Markets#show</h1>
<section>
	<p>id: <%= @mymarket.id %></p>
	<p>name: <%= @mymarket.name %></p>
	<p>address: <%=@mymarket.address %> <%=@mymarket.city%> <%=@mymarket.county%> <%=@mymarket.state%>, <%=@mymarket.zip%></p>
  <p><%=link_to("delete", markets_delete_path(@mymarket.id), method: :delete)%></p>
</section>
<section>
	<h2>Vendors</h2>
	<% @myvendors.each do |vendor| %>
		<p><%= vendor.name %></p>
	<%end%>
</section>
